{
  "name": "lucid-ui",
  "version": "4.0.0-beta.13",
  "description": "A UI component library from AppNexus.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/lucid.git"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-modules",
    "build-bundle": "scripts/build-bundle.sh",
    "build-docs": "rm -rf ./dist/docs && NODE_DEST_ENV=docs build-storybook -s ./docs/public -o ./dist/docs",
    "build-js": "NODE_ENV=production babel --extensions .js,.jsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./dist",
    "build-modules": "NODE_ENV=production BABEL_ENV=modules babel --extensions .js,.jsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./lib",
    "cover": "npm run jest -- --coverage",
    "dev": "npm run storybook",
    "docs": "NODE_ENV=development start-storybook -s ./docs/public -p 6006",
    "jest": "NODE_ENV=test TZ=UTC jest",
    "snapshot": "NODE_ENV=test TZ=UTC jest -u",
    "lint": "eslint ./src --ext .js,.jsx",
    "lint-fix": "eslint ./src --ext .js,.jsx --fix",
    "postpublish": "npm run build-docs && scripts/upload-docs-build-gh.sh && scripts/upload-docs-build-docspot.sh",
    "prettier": "prettier --write '{src,tests,scripts}/**/*.{js,jsx}'",
    "preversion": "rm -rf dist && rm -rf lib && npm run lint && npm run build-js && npm run build-bundle && npm run build-modules",
    "release": "np",
    "start": "npm run docs",
    "storybook": "start-storybook -p 6006",
    "stylelint": "stylelint 'src/**/*.less' --config .stylelintrc",
    "test": "npm run lint && npm run cover",
    "upload-docs": "npm run build-docs && scripts/upload-docs-build-docspot.sh",
    "watch": "npm run jest -- --watch"
  },
  "keywords": [
    "component",
    "ecosystem-react",
    "react",
    "react-component",
    "reactive",
    "stateful",
    "stateless"
  ],
  "author": "AppNexus engineering",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.2.2",
    "classnames": "^2.2.3",
    "create-react-class": "^15.6.2",
    "d3-format": "^1.0.2",
    "d3-scale": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3-time": "^1.0.0",
    "d3-time-format": "^2.0.0",
    "element-resize-detector": "^1.1.6",
    "lodash": "^4.11.0",
    "prop-types": "^15.6.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-day-picker": "^6.2.1",
    "react-motion": "^0.5.2",
    "react-peek": "^1.0.5",
    "react-transition-group": "^1.2.1",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-info": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addon-options": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "autoprefixer": "^8.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^5.16.0",
    "eslint-config-appnexus": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-lodash": "^2.3.4",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "glob": "^7.1.1",
    "husky": "^2.2.0",
    "jest": "^21.2.1",
    "jscodeshift": "^0.5.0",
    "less": "^2.3.1",
    "less-loader": "^4.1.0",
    "marksy": "^6.0.3",
    "mini-css-extract-plugin": "^0.4.0",
    "np": "^2.20.1",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.17.0",
    "raw-loader": "^0.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.0",
    "regenerator-runtime": "^0.13.1",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.20.3",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "jest": {
    "rootDir": "src",
    "setupFiles": [
      "../tests/init",
      "../tests/enzyme-setup"
    ],
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}",
      "!components/**/examples/*",
      "util/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  }
}
