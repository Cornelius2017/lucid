{
  "name": "lucid-ui",
  "version": "4.1.0",
  "description": "A UI component library from AppNexus.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/lucid.git"
  },
  "scripts": {
    "build:bundle:development": "NODE_ENV=development webpack --config webpack.config.bundle.js",
    "build:bundle:minified": "NODE_ENV=production webpack --config webpack.config.bundle.js",
    "build:docs": "rm -rf ./dist/docs && NODE_DEST_ENV=docs build-storybook -s ./docs/public -o ./dist/docs",
    "build:js": "NODE_ENV=production babel --extensions .js,.jsx,.ts,.tsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./dist",
    "build:modules": "NODE_ENV=production BABEL_ENV=modules babel --extensions .js,.jsx,.ts,.tsx --ignore spec.js,docs,examples,test-entry.js,coverage ./src --out-dir ./lib",
    "clean:dist": "rm -rf dist",
    "clean:lib": "rm -rf lib",
    "cover": "npm run jest -- --coverage",
    "dev": "start-storybook -p 6006",
    "docs:upload:docspot": "scripts/upload-docs-build-docspot.sh",
    "docs:upload:gh": "scripts/upload-docs-build-gh.sh",
    "jest": "NODE_ENV=test TZ=UTC jest",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "postpublish": "npm-run-all build:docs --parallel docs:upload:*",
    "prepublishOnly": "npm-run-all clean:* lint --parallel build:js build:modules build:bundle:*",
    "prettier": "prettier --write '{src,tests,scripts}/**/*.{js,jsx}'",
    "snapshot": "NODE_ENV=test TZ=UTC jest -u",
    "stylelint": "stylelint 'src/**/*.less' --config .stylelintrc",
    "test": "run-s lint cover",
    "upload-docs": "run-s build:docs docs:upload:docspot",
    "watch": "npm run jest -- --watch"
  },
  "keywords": [
    "component",
    "ecosystem-react",
    "react",
    "react-component",
    "reactive",
    "stateful",
    "stateless"
  ],
  "author": "AppNexus engineering",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.2.2",
    "classnames": "^2.2.3",
    "create-react-class": "^15.6.2",
    "d3-format": "^1.0.2",
    "d3-scale": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3-time": "^1.0.0",
    "d3-time-format": "^2.0.0",
    "element-resize-detector": "^1.1.6",
    "prop-types": "^15.6.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-day-picker": "^6.2.1",
    "react-motion": "^0.5.2",
    "react-peek": "^1.0.5",
    "react-transition-group": "^1.2.1",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-info": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addon-options": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "@types/create-react-class": "^15.6.2",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^8.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-lodash": "^2.3.4",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "glob": "^7.1.1",
    "husky": "^3.0.0",
    "jest": "^21.2.1",
    "jscodeshift": "^0.5.0",
    "less": "^2.3.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.14",
    "marksy": "^6.0.3",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.17.0",
    "raw-loader": "^0.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.0",
    "regenerator-runtime": "^0.13.1",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.20.3",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "jest": {
    "rootDir": "src",
    "setupFiles": [
      "../tests/init",
      "../tests/enzyme-setup"
    ],
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}",
      "!components/**/examples/*",
      "util/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
